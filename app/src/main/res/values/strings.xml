    <!-- Single Emotions Descriptions -->
    <!-- Joy Group -->
    <string name="emotion_detail_ecstasy_desc">기쁨이 극에 달해 세상을 다 가진 듯 벅차오르는 황홀경입니다.</string>
    <string name="emotion_detail_joy_desc">마음이 즐겁고 행복하여 저절로 미소가 지어지는 상태입니다.</string>
    <string name="emotion_detail_serenity_desc">파도 없는 바다처럼 마음이 고요하고 평화로운 상태입니다.</string>

    <!-- Trust Group -->
    <string name="emotion_detail_admiration_desc">상대방의 훌륭함에 깊이 감동하여 마음 깊이 존경하는 상태입니다.</string>
    <string name="emotion_detail_trust_desc">상대방이나 상황을 의심 없이 믿고 의지하는 든든한 마음입니다.</string>
    <string name="emotion_detail_acceptance_desc">상황을 있는 그대로 받아들이고 인정하는 열린 마음입니다.</string>

    <!-- Fear Group -->
    <string name="emotion_detail_terror_desc">압도적인 공포로 인해 온몸이 얼어붙고 패닉에 빠진 상태입니다.</string>
    <string name="emotion_detail_fear_desc">위협을 느껴 마음이 불안하고 도망치고 싶은 상태입니다.</string>
    <string name="emotion_detail_apprehension_desc">나쁜 일이 일어날까 봐 마음이 조마조마하고 걱정되는 상태입니다.</string>

    <!-- Surprise Group -->
    <string name="emotion_detail_amazement_desc">예상치 못한 엄청난 일에 입이 다물어지지 않을 정도로 크게 놀란 상태입니다.</string>
    <string name="emotion_detail_surprise_desc">갑작스러운 일로 인해 순간적으로 당황하거나 놀란 상태입니다.</string>
    <string name="emotion_detail_distraction_desc">주의가 산만해져서 한 곳에 집중하기 어렵고 마음이 붕 뜬 상태입니다.</string>

    <!-- Sadness Group -->
    <string name="emotion_detail_grief_desc">가슴이 찢어질 듯한 깊은 슬픔에 빠져 헤어나오기 힘든 상태입니다.</string>
    <string name="emotion_detail_sadness_desc">마음이 무겁고 우울하여 기운이 없고 눈물이 나는 상태입니다.</string>
    <string name="emotion_detail_pensiveness_desc">깊은 생각에 잠겨 약간의 우수와 쓸쓸함을 느끼는 상태입니다.</string>

    <!-- Disgust Group -->
    <string name="emotion_detail_loathing_desc">도저히 참을 수 없을 만큼 강한 혐오감을 느끼는 상태입니다.</string>
    <string name="emotion_detail_disgust_desc">대상이 불쾌하고 역겨워 멀리하고 싶은 마음입니다.</string>
    <string name="emotion_detail_boredom_desc">특별한 자극이 없어 따분하고 지루함을 느끼는 상태입니다.</string>

    <!-- Anger Group -->
    <string name="emotion_detail_rage_desc">화가 머리끝까지 치밀어 올라 폭발할 것 같은 격렬한 분노입니다.</string>
    <string name="emotion_detail_anger_desc">불만이나 부당함으로 인해 화가 나고 기분이 상한 상태입니다.</string>
    <string name="emotion_detail_annoyance_desc">상황이나 사람이 거슬려 마음이 약간 불편하고 짜증이 나는 상태입니다.</string>

    <!-- Anticipation Group -->
    <string name="emotion_detail_vigilance_desc">위험이나 기회에 대비하여 신경을 곤두세우고 경계하는 상태입니다.</string>
    <string name="emotion_detail_anticipation_desc">앞으로 일어날 일에 대해 설레는 마음으로 기다리는 상태입니다.</string>
    <string name="emotion_detail_interest_desc">대상이나 상황에 호기심을 느끼고 주의를 기울이는 상태입니다.</string>

    <!-- Complex Emotions (Dyads) -->
    <!-- Primary -->
    <string name="emotion_complex_love_title">사랑 (Love)</string>
    <string name="emotion_complex_love_desc">기쁨과 신뢰가 만나 따뜻하고 깊은 애정을 형성합니다.</string>
    
    <string name="emotion_complex_submission_title">수용 (Submission)</string>
    <string name="emotion_complex_submission_desc">신뢰와 두려움이 섞여 어쩔 수 없는 상황을 받아들이거나 순응하는 마음입니다.</string>
    
    <string name="emotion_complex_awe_title">경외감 (Awe)</string>
    <string name="emotion_complex_awe_desc">두려움과 놀람이 결합되어 압도적이거나 신비로운 대상을 향한 느낌입니다.</string>
    
    <string name="emotion_complex_disapproval_title">실망 (Disapproval)</string>
    <string name="emotion_complex_disapproval_desc">놀람과 슬픔이 만나, 예상치 못한 부정적 결과에 대해 불만을 가집니다.</string>
    
    <string name="emotion_complex_remorse_title">후회 (Remorse)</string>
    <string name="emotion_complex_remorse_desc">슬픔과 혐오가 섞여 과거의 행동이나 자신에 대한 깊은 자책감을 만듭니다.</string>
    
    <string name="emotion_complex_contempt_title">경멸 (Contempt)</string>
    <string name="emotion_complex_contempt_desc">혐오와 분노가 결합되어 대상을 낮잡아보거나 강한 거부감을 보입니다.</string>
    
    <string name="emotion_complex_aggressiveness_title">공격성 (Aggressiveness)</string>
    <string name="emotion_complex_aggressiveness_desc">분노와 기대가 만나 목표를 향해 거칠게 나아가거나 도전하려는 에너지입니다.</string>
    
    <string name="emotion_complex_optimism_title">낙관 (Optimism)</string>
    <string name="emotion_complex_optimism_desc">기대와 기쁨이 어우러져 미래가 잘 풀릴 것이라는 긍정적인 희망을 가집니다.</string>
    
    <!-- Secondary -->
    <string name="emotion_complex_guilt_title">죄책감 (Guilt)</string>
    <string name="emotion_complex_guilt_desc">기쁨을 누리는 것 같으면서도 두려움이 개입되어 느끼는 마음의 불편함입니다.</string>
    
    <string name="emotion_complex_curiosity_title">호기심 (Curiosity)</string>
    <string name="emotion_complex_curiosity_desc">믿음 안에서 새로운 것에 대해 놀라움을 느끼며 탐구하려는 마음입니다.</string>
    
    <string name="emotion_complex_despair_title">절망 (Despair)</string>
    <string name="emotion_complex_despair_desc">두려움과 슬픔이 만나 희망을 잃고 깊은 어둠에 빠진 상태입니다.</string>
    
    <string name="emotion_complex_unbelief_title">불신 (Unbelief)</string>
    <string name="emotion_complex_unbelief_desc">놀람과 혐오가 섞여 상황을 믿을 수 없어 하거나 거부하는 태도입니다.</string>
    
    <string name="emotion_complex_envy_title">질투 (Envy)</string>
    <string name="emotion_complex_envy_desc">슬픔과 분노가 섞여 타인의 행복을 보며 자신의 결핍에 괴로워합니다.</string>
    
    <string name="emotion_complex_cynicism_title">냉소 (Cynicism)</string>
    <string name="emotion_complex_cynicism_desc">혐오와 기대가 기묘하게 섞여, 결과에 대해 부정적으로 예측하고 비웃는 태도입니다.</string>
    
    <string name="emotion_complex_pride_title">자부심 (Pride)</string>
    <string name="emotion_complex_pride_desc">분노(자아 주장)와 기쁨이 결합되어 자신의 성취나 가치에 대해 강한 확신을 가집니다.</string>
    
    <string name="emotion_complex_fatalism_title">숙명 (Fatalism)</string>
    <string name="emotion_complex_fatalism_desc">기대와 신뢰가 섞여 미래의 일을 정해진 것으로 받아들이는 태도입니다.</string>
    
    <!-- Tertiary -->
    <string name="emotion_complex_delight_title">환희 (Delight)</string>
    <string name="emotion_complex_delight_desc">기쁨과 놀람이 만나 예상치 못한 즐거움에 크게 기뻐하는 상태입니다.</string>
    
    <string name="emotion_complex_sentimentality_title">감상 (Sentimentality)</string>
    <string name="emotion_complex_sentimentality_desc">신뢰와 슬픔이 섞여 지나간 인연이나 추억에 젖어드는 마음입니다.</string>
    
    <string name="emotion_complex_shame_title">수치심 (Shame)</string>
    <string name="emotion_complex_shame_desc">두려움과 혐오가 만나 자신의 모습에 대해 부끄러움을 느끼고 숨고 싶은 마음입니다.</string>
    
    <string name="emotion_complex_outrage_title">격분 (Outrage)</string>
    <string name="emotion_complex_outrage_desc">놀람과 분노가 섞여 예상치 못한 일에 대해 강하게 화를 내는 상태입니다.</string>
    
    <string name="emotion_complex_pessimism_title">비관 (Pessimism)</string>
    <string name="emotion_complex_pessimism_desc">슬픔과 기대가 섞여 미래에 대해 부정적인 결과를 예상하고 미리 슬퍼하는 마음입니다.</string>
    
    <string name="emotion_complex_morbidness_title">병적 흥미 (Morbidness)</string>
    <string name="emotion_complex_morbidness_desc">혐오스러운 대상에게서 묘한 기쁨을 느끼는 복잡한 심리입니다.</string>
    
    <string name="emotion_complex_dominance_title">지배 (Dominance)</string>
    <string name="emotion_complex_dominance_desc">분노와 신뢰가 만나 자신의 주장을 믿고 타인을 통제하려는 태도입니다.</string>
    
    <string name="emotion_complex_anxiety_title">불안 (Anxiety)</string>
    <string name="emotion_complex_anxiety_desc">기대와 두려움이 섞여 다가올 미래에 대해 알 수 없어 초조해하는 마음입니다.</string>

